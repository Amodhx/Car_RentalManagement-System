<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="274.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.innovestaproject.PassWordChangeController">
    <children>
        <Label layoutX="20.0" layoutY="58.0" text="Enter your user name" />
        <Label layoutX="20.0" layoutY="129.0" text="Enter your new Password" />
        <TextField fx:id="un" layoutX="20.0" layoutY="86.0" prefHeight="26.0" prefWidth="308.0" promptText="User Name" />
        <TextField fx:id="pw" layoutX="20.0" layoutY="153.0" prefHeight="26.0" prefWidth="308.0" promptText="Pass Word" />
        <AnchorPane layoutY="-4.0" prefHeight="34.0" prefWidth="350.0" style="-fx-background-color: #0FF0F0;">
            <children>
                <Label layoutX="112.0" layoutY="8.0" text="Change Credential">
                    <font>
                        <Font name="Bell MT" size="14.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Button fx:id="button2" layoutX="106.0" layoutY="204.0" mnemonicParsing="false" onAction="#onButton2Click" style="-fx-background-radius: 10;" text="Cancel" />
        <Button fx:id="button" defaultButton="true" layoutX="184.0" layoutY="204.0" mnemonicParsing="false" onAction="#OnButtonClick" prefHeight="24.0" prefWidth="54.0" style="-fx-background-radius: 10;" text="OK" />
        <Label fx:id="l1" alignment="CENTER" layoutX="15.0" layoutY="222.0" prefHeight="18.0" prefWidth="239.0" textFill="#ed0c0e" />
    </children>
</AnchorPane>
