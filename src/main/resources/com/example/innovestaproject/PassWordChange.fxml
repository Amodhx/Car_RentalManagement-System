<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="416.0" prefWidth="269.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.innovestaproject.PassWordChangeController">
    <children>
        <Label layoutX="20.0" layoutY="58.0" text="Enter your user name:&gt;" />
        <Label layoutX="20.0" layoutY="145.0" text="Enter your new Password:&gt;" />
        <TextField fx:id="un" layoutX="20.0" layoutY="86.0" prefHeight="26.0" prefWidth="239.0" promptText="User Name" />
        <TextField fx:id="pw" layoutX="20.0" layoutY="169.0" prefHeight="26.0" prefWidth="239.0" promptText="Pass Word" />
        <AnchorPane layoutY="4.0" prefHeight="26.0" prefWidth="269.0" style="-fx-background-color: #0FF0F0;">
            <children>
                <Label layoutX="82.0" layoutY="8.0" text="Change Credential">
                    <font>
                        <Font name="Bell MT" size="14.0" />
                    </font>
                </Label>
            </children>
        </AnchorPane>
        <Button fx:id="button2" layoutX="118.0" layoutY="286.0" mnemonicParsing="false" onAction="#onButton2Click" text="Cancel" />
        <Button fx:id="button" defaultButton="true" layoutX="210.0" layoutY="286.0" mnemonicParsing="false" onAction="#OnButtonClick" text="OK" />
        <Label fx:id="l1" alignment="CENTER" layoutX="15.0" layoutY="222.0" prefHeight="18.0" prefWidth="239.0" textFill="#ed0c0e" />
    </children>
</AnchorPane>
